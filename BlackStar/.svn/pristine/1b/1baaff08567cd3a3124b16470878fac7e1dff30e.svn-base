/**
 * 시계 상세 정보
 * 
 * @param value
 * @param ItemName
 * @returns
 */
function Watch(value, ItemName) {

	// 상품 정보 데이터 저장 Array
	var categoryArr = {
		"num" : value,
		"item" : ItemName
	};
	window.location.href = "/ItemCategory/itemDetail.item?item="+
	categoryArr.num + categoryArr.item;

};